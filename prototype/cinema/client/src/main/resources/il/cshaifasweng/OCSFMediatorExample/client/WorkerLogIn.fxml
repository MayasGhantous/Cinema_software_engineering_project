<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.WorkerLogInController">
   <children>
      <Button fx:id="LogInBut" layoutX="274.0" layoutY="257.0" mnemonicParsing="false" onAction="#LogIn" text="Log in" />
      <TextField id="userName" fx:id="userNameTF" layoutX="226.0" layoutY="123.0" onAction="#checkUserName" promptText="User name:" />
      <TextField id="password" fx:id="passwordTF" layoutX="227.0" layoutY="175.0" onAction="#checkPass" promptText="Password:" />
      <Text fx:id="output" layoutX="228.0" layoutY="50.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" wrappingWidth="149.5999999999999" />
   </children>
</AnchorPane>
