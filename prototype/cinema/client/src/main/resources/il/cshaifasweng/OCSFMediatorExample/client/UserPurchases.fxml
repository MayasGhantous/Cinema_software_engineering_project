<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.UserPurchasesController">
   <children>
      <TableView fx:id="table_view" layoutX="1.0" layoutY="179.0" prefHeight="225.0" prefWidth="600.0">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
      <Button fx:id="show_movies" layoutX="42.0" layoutY="17.0" mnemonicParsing="false" onAction="#show_movies_func" text="Show" />
      <Button fx:id="delete_movie" layoutX="448.0" layoutY="69.0" mnemonicParsing="false" onAction="#deleteMovie" text="delete" textFill="#f50202">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="select_movie" layoutX="277.0" layoutY="69.0" mnemonicParsing="false" onAction="#selectMovie" text="select" />
   </children>
</AnchorPane>
